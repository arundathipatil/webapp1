[Unit]
Description=webapp

[Service]
EnvironmentFile=/etc/environment
User=ubuntu
ExecStartPre= rm -rf /home/ubuntu/webapp.log
ExecStart=/usr/bin/java -jar /home/ubuntu/webapp/BE-layer-1.0-SNAPSHOT.jar
SuccessExitStatus=143
Restart=always
RestartSec=5
StandardOutput=file:/home/ubuntu/webapp.log

[Install]
WantedBy=multi-user.target